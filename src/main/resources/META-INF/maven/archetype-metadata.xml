            <directory>src/main/java</directory>
            <includes>
                <include>**/*.java</include>
            </includes>
        </fileSet>
        <fileSet filtered="true" packaged="false">
            <directory>res</directory>
            <includes>
                <include>*.properties</include>
            </includes>
        </fileSet>
        <fileSet filtered="true" packaged="false">
            <directory>src/test/java</directory>
            <includes>
                <include>**/*.java</include>
            </includes>
        </fileSet>
    </fileSets>
